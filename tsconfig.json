{
    "compilerOptions": {
        "target": "es6",
        "sourceMap": true,
        "forceConsistentCasingInFileNames": true,
        "noImplicitReturns": true,
        "strict": true,
        "noImplicitAny": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true
    },
    "typeRoots": ["node_modules/@types"],
    "exclude": ["node_modules", "typings"]
}
